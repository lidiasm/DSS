<bpel:process name="Empleado"
         targetNamespace="http://Empleado"
         suppressJoinFailure="yes"
         xmlns:tns="http://Empleado"
         xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         >

    <!-- Import the client WSDL -->
	<bpel:import location="EmpleadoArtifacts.wsdl" namespace="http://Empleado" 
	        importType="http://schemas.xmlsoap.org/wsdl/" />
         
    <!-- ================================================================= -->         
    <!-- PARTNERLINKS                                                      -->
    <!-- List of services participating in this BPEL process               -->
    <!-- ================================================================= -->         
    <bpel:partnerLinks>
        <!-- The 'client' role represents the requester of this service. -->
        <bpel:partnerLink name="client"
                     partnerLinkType="tns:Empleado"
                     myRole="EmpleadoProvider"
                     />
    </bpel:partnerLinks>
  
    <!-- ================================================================= -->         
    <!-- VARIABLES                                                         -->
    <!-- List of messages and XML documents used within this BPEL process  -->
    <!-- ================================================================= -->         
    <bpel:variables>
        <!-- Reference to the message passed as input during initiation -->
        <bpel:variable name="input"
                  messageType="tns:EmpleadoRequestMessage"/>
                  
        <!-- 
          Reference to the message that will be returned to the requester
          -->
        <bpel:variable name="output"
                  messageType="tns:EmpleadoResponseMessage"/>
    </bpel:variables>

    <!-- ================================================================= -->         
    <!-- ORCHESTRATION LOGIC                                               -->
    <!-- Set of activities coordinating the flow of messages across the    -->
    <!-- services integrated within this business process                  -->
    <!-- ================================================================= -->         
    <bpel:sequence name="main">
        
        <!-- Receive input from requester. 
             Note: This maps to operation defined in Empleado.wsdl 
             -->
        <bpel:receive name="receiveInput" partnerLink="client" variable="input"
                 createInstance="yes" operation="getClaseViaje" portType="tns:Empleado"/>
        
        <!-- Generate reply to synchronous request -->
        <bpel:if name="Status">
            <bpel:condition expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[$input.payload/tns:nombre = "Lidia"]]></bpel:condition>
            <bpel:assign validate="no" name="AssignTurista">
                <bpel:copy>
                    <bpel:from>
                        <bpel:literal xml:space="preserve">
                        	<tns:EmpleadoResponse xmlns:tns="http://Empleado" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  								<tns:clase>turista</tns:clase>
  							</tns:EmpleadoResponse>
						</bpel:literal>
                    </bpel:from>
                    <bpel:to part="payload" variable="output"></bpel:to>
                </bpel:copy>
            </bpel:assign>
        <bpel:elseif>
        	<bpel:condition expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[$input.payload/tns:nombre="Eva"]]></bpel:condition>
            <bpel:assign validate="no" name="AssignBusiness">
                <bpel:copy>
                    <bpel:from>
						<bpel:literal xml:space="preserve">
                        	<tns:EmpleadoResponse xmlns:tns="http://Empleado" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  								<tns:clase>business</tns:clase>
  							</tns:EmpleadoResponse>
						</bpel:literal>
                    </bpel:from>
                    <bpel:to part="payload" variable="output"></bpel:to>
                </bpel:copy>
            </bpel:assign>
        </bpel:elseif>
        <bpel:else>
            <bpel:assign validate="no" name="AssignJetPrivado">
                <bpel:copy>
                    <bpel:from>
                        <bpel:literal xml:space="preserve">
                        	<tns:EmpleadoResponse xmlns:tns="http://Empleado" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  								<tns:clase>jetprivado</tns:clase>
  							</tns:EmpleadoResponse>
						</bpel:literal>
                    </bpel:from>
                    <bpel:to part="payload" variable="output"></bpel:to>
                </bpel:copy>
            </bpel:assign>
        </bpel:else></bpel:if>
        <bpel:reply name="replyOutput" 
               partnerLink="client"
               portType="tns:Empleado"
               operation="getClaseViaje" 
               variable="output"
               />
    </bpel:sequence>
</bpel:process>

