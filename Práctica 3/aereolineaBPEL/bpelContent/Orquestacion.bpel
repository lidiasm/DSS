                </bpel:copy>
                <bpel:copy>
                    <bpel:from><bpel:literal><tns:VuelingResponse xmlns:tns="http://Vueling" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:Precio>0.0</tns:Precio>
