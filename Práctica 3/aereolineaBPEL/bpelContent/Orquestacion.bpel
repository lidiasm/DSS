                </bpel:to>
            </bpel:copy>
        </bpel:assign>
        <bpel:invoke name="Iberia" partnerLink="iberia" operation="getPrecioViaje" portType="ns2:Iberia" inputVariable="IberiaRequest" outputVariable="IberiaResponse"></bpel:invoke>
