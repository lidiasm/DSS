                    </bpel:from>
                    <bpel:to variable="VuelingResponse">
                        <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[ns3:Precio]]></bpel:query>
